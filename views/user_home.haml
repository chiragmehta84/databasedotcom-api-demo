.search
  %form{:action => '/search'}
    .search_box
      %input{:type => "textfield", :name => "search"}
    .search_button
      %input{:type => "submit", :value => "SOSL Search"}

%h2 Classes in the database
.page
  %ul
    - session[:client].list_sobjects.each do |sobject|
      %li
        .sobject
          %a{:href => "/sobject/#{sobject}" }
            = sobject